}

.member1 {
    float: left;
    display: table-cell;
    width: 33%;
}
.member1 h5{
	background: #01bf9d;
    font: bold 14px/0px 'Open Sans', sans-serif;
    text-align: center;
    padding: 20px;
    margin: 20px 20px 0 0;
    color: #fff;
	}
	.member1 p {
    padding: 0;
    margin: 7px;
    -webkit-margin-before: 0;
    -moz-margin-before: 0;
	font: bold 14px/20px 'Open Sans', sans-serif;
}
	.member1 p span {
    float: right;
    margin: 0 25px;
   
}
	.head {
    background: #e0e0e0;
    padding: 15px 0 0 10px;
    margin-right: 20px;
	box-shadow: inset 0px 1px 10px #005c4c;
	height:150px;
}

