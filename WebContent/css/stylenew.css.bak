.pop_up {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, .7);
    -ms-transform: scale(1) rotate(0) translate(0, 0) skew(0, 0);
    transform: scale(1) rotate(0) translate(0, 0) skew(0, 0);
    transition-property: transform, opacity;
    transition-duration: .3s;
	z-index:999;
}

div#popup_41 {
    width: 30%;
    height: auto;
    top: 24%;
    left: 13%;
    position: fixed;
    color: #fff;
    padding: 7px 5px 3px 5px;
	background:rgba(0, 0, 0, 0.9);
	 -ms-transform: scale(1) rotate(0) translate(0, 0) skew(0, 0);
    transform: scale(1) rotate(0) translate(0, 0) skew(0, 0);
    transition-property: transform, opacity;
    transition-duration: .3s;
	z-index:999;
}
.popup_hide {
    opacity: 0;
    -ms-transform: scale(0.5) rotate(0) translate(0, -200%) skew(0, 0);
    transform: scale(0.5) rotate(0) translate(0, -200%) skew(0, 0);
    /*if you want to change popup view init show way*/
}

div#popup_41 .pop_up_close{
	margin:7px 10px 0 0;
	cursor:pointer;
	
}
div#popup_41 .pop_up_close:before{
	background:#fff;
}
div#popup_41 .pop_up_close:after{
	background:#fff;
}
.pop_up_close {
    position: absolute;
    top: 0;
    right: 0;
    margin: 20px 20px 0 0;
    width: 32px;
    height: 32px;
    cursor: pointer;
}
.pop_up_close:before {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 5px;
    background: #000;
    -ms-transform: rotate(45deg) translate(10px, 10px);
    transform: rotate(45deg) translate(10px, 10px);
}
.pop_up_close:after {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 5px;
    background: #000;
    -ms-transform: rotate(-45deg) translate(-10px, 10px);
    transform: rotate(-45deg) translate(-10px, 10px);
}

